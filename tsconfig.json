        "typings/node-uuid/node-uuid-base.d.ts",
        "typings/node-uuid/node-uuid-cjs.d.ts",
        "typings/node-uuid/node-uuid-global.d.ts",
        "typings/node-uuid/node-uuid.d.ts",
        "typings/node/node.d.ts",
